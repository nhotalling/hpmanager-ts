{
  "name": "hp-manager-express",
  "version": "1.0.0",
  "description": "Hit Point Manager",
  "scripts": {
    "build": "tsc",
    "watch": "nodemon -e ts -w ./src -x npm run dev",
    "dev": "ts-node ./src/app/server.ts",
    "start": "nodemon ./dist/server.js",
    "prod": "npm run build && npm run start",
    "test": "mocha -r ts-node/register \"./src/app/tests/**/*.test.ts\""
  },
  "author": "Nick Hotalling",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.7",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.13.0",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
